<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="hh_fi_fi">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="hh_fi_wr">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="hh_wr_wr">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <!--基本Style结束-->
    <!--权重开始-->

    <!--权重结束-->
    <!--字体Style开始-->
    <style name="text_black_16_f_w" parent="@style/hh_fi_wr">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black_text</item>
    </style>

    <style name="text_black_16_w_w" parent="@style/hh_wr_wr">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black_text</item>
    </style>

    <style name="text_gray_16_f_w" parent="@style/text_black_16_f_w">
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="text_gray_16_w_w" parent="@style/text_black_16_w_w">
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="text_black_14_f_w" parent="@style/text_black_16_f_w">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_white_14_f_w" parent="@style/text_white_16_f_w">
        <item name="android:textSize">14sp</item>
    </style>


    <style name="text_black_14_w_w" parent="@style/text_black_16_w_w">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_gray_14_f_w" parent="@style/text_black_14_f_w">
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="text_gray_14_w_w" parent="@style/text_black_14_w_w">
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="text_gray_14_w_w_666" parent="@style/text_black_14_w_w">
        <item name="android:textColor">@color/green_text_666</item>
    </style>

<!--    <style name="text_gray_14_w_w_999" parent="@style/text_black_14_w_w">-->
<!--        <item name="android:textColor">@color/green_text_999</item>-->
<!--    </style>-->

    <style name="text_black_12_f_w" parent="@style/text_black_16_f_w">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_black_12_w_w" parent="@style/text_black_16_w_w">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_gray_12_f_w" parent="@style/text_black_12_f_w">
        <item name="android:textColor">@color/gray_text</item>
    </style>

    <style name="text_gray_12_w_w" parent="@style/text_black_12_w_w">
        <item name="android:textColor">@color/gray_text</item>
    </style>
    <!--白色-->
    <style name="text_white_16_f_w" parent="@style/hh_fi_wr">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text_white_16_w_w" parent="@style/hh_wr_wr">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text_white_18_w_w" parent="@style/text_white_16_w_w">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="text_white_14_w_w" parent="@style/hh_wr_wr">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="text_white_12_w_w" parent="@style/hh_wr_wr">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
    </style>


    <style name="text_black_18_w_w" parent="@style/hh_wr_wr">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black_text</item>
    </style>
    <!--字体Style结束-->

    <!--scrollView开始-->
    <style name="scrollView" parent="@style/hh_fi_fi">
        <item name="android:scrollbars">none</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fadingEdgeLength">0dp</item>
        <item name="android:fillViewport">true</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <!--scrollView结束-->

    <!--ListView开始-->
    <style name="listView" parent="@style/hh_fi_fi">
        <item name="android:listSelector">@color/transparent</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fadingEdgeLength">0dp</item>
        <item name="android:scrollbars">none</item>
        <item name="android:fadeScrollbars">false</item>
        <item name="android:dividerHeight">0dp</item>
        <item name="android:cacheColorHint">@color/transparent</item>
        <item name="android:divider">@color/background</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <!--ListView结束-->


    <!--RecyclerView开始-->
    <style name="recyclerView" parent="@style/hh_fi_fi">
        <item name="android:scrollbars">none</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <!--RecyclerView开始-->

    <!--1dp分割线开始-->
    <style name="line">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/background</item>
    </style>
    <style name="line_goods">
        <item name="android:layout_height">0.5dp</item>
        <item name="android:layout_marginStart">8.5dp</item>
        <item name="android:layout_marginEnd">9dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@color/background</item>
    </style>
    <!--1dp分割线结束-->

    <!--EditText Style开始-->
    <style name="et_base_style_simple" parent="text_black_14_f_w">
        <item name="android:inputType">text</item>
        <item name="android:textColorHint">@color/gray_text</item>
    </style>

    <style name="et_base_style" parent="text_black_14_f_w">
        <item name="android:inputType">text</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColorHint">@color/gray_text</item>
    </style>

    <style name="et_base_style_no_bg" parent="et_base_style">
        <item name="android:background">@null</item>
    </style>


    <style name="et_base_style_no_bg_wr_wr" parent="text_black_14_w_w">
        <item name="android:inputType">text</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColorHint">@color/gray_text</item>
        <item name="android:background">@null</item>
    </style>
    <!--EditText Style结束-->
    <!--项目中用到的Style结束-->

</resources>