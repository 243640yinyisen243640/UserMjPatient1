<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <com.wei.android.lib.colorview.view.ColorLinearLayout
        style="@style/hh_wr_wr"
        android:layout_width="285dp"
        android:orientation="vertical"
        app:backgroundColorNormal="@color/white"
        app:cornerRadiusNormal="@dimen/dp_15">

        <LinearLayout
            style="@style/hh_fi_wr"
            android:layout_margin="@dimen/dp_16">

            <TextView
                android:id="@+id/tv_title"
                style="@style/text_black_16_f_w"
                android:layout_weight="0.8"
                android:text="尿常规报告" />

            <TextView
                android:id="@+id/tv_time"
                style="@style/text_gray_14_w_w"
                android:text="2019 09 10 14:25" />

        </LinearLayout>


        <ListView
            android:id="@+id/lv_check_data_blood_common"
            style="@style/listView"
            android:visibility="visible">

        </ListView>


    </com.wei.android.lib.colorview.view.ColorLinearLayout>


</LinearLayout>