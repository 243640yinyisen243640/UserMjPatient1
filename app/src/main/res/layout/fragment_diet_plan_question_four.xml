<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/include_diet_plan_question_title_and_desc" />

    <GridView
        android:id="@+id/gv_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_40"
        android:background="@color/white"
        android:horizontalSpacing="@dimen/dp_12"
        android:numColumns="2"
        android:paddingLeft="@dimen/dp_12"
        android:paddingRight="@dimen/dp_12"
        android:paddingBottom="@dimen/dp_12"
        android:verticalSpacing="@dimen/dp_12"
        android:visibility="visible">

    </GridView>

    <com.wei.android.lib.colorview.view.ColorTextView
        android:id="@+id/tv_next"
        style="@style/diet_plan_question_tv_next"
        android:layout_marginTop="@dimen/dp_40"
        android:text="下一步"
        android:textColor="@color/white_text"
        app:backgroundColorNormal="@color/main_home"
        app:cornerRadiusNormal="3dp">

    </com.wei.android.lib.colorview.view.ColorTextView>

</LinearLayout>